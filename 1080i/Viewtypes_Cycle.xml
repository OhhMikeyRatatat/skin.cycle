<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<includes>
	
	<include name="Viewtypes_Cycle">
		<control type="group">
			<!-- Home Static Group -->
			<visible>Control.IsVisible(51)</visible>
			<include>Home-BGFade</include>
			<control type="group">
				<posy>110</posy>
				<control type="label">
					<posx>10</posx>
					<posy>120</posy>
					<width>620</width>
					<align>center</align>
					<font>Font-Home-NavigationText-S</font>
					<textcolor>Home-NavigationText</textcolor>
					<label>$INFO[Container(51).ListItem(-1).Label]</label>
					<wrapmultiline>true</wrapmultiline>
					<animation effect="fade" end="50" condition="true">Conditional</animation>
				</control>
				<control type="label">
					<posx>650</posx>
					<posy>0</posy>
					<width>620</width>
					<align>center</align>
					<font>Font-Home-NavigationText</font>
					<textcolor>Home-NavigationText</textcolor>
					<wrapmultiline>true</wrapmultiline>
					<label>$INFO[Container(51).ListItem.Label]</label>
				</control>
				<control type="label">
					<posx>1290</posx>
					<posy>120</posy>
					<width>620</width>
					<align>center</align>
					<font>Font-Home-NavigationText-S</font>
					<textcolor>Home-NavigationText</textcolor>
					<label>$INFO[Container(51).ListItem(1).Label]</label>
					<wrapmultiline>true</wrapmultiline>
					<animation effect="fade" end="50" condition="true">Conditional</animation>
				</control>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>251</posy>
				<width>1920</width>
				<height>578</height>
				<texture>views/cycle/bar.png</texture>
			</control>
		</control>
		<control type="wraplist" id="51">
			<posx>-690</posx>
			<posy>310</posy>
			<width>2500</width>
			<height>460</height>
			<onleft>51</onleft>
			<onright>51</onright>
			<onup>9000</onup>
			<ondown>9000</ondown>
			<pagecontrol>60</pagecontrol>
			<focusposition>2</focusposition>
			<viewtype label="Cycle">list</viewtype>
			<scrolltime tween="cubic" easing="inout">450</scrolltime>
			<orientation>horizontal</orientation>
			
			<animation effect="slide" condition="true" end="100">Conditional</animation>
			<itemlayout width="640">
				<control type="group">
					<animation effect="zoom" end="75" condition="true" center="270,230">Conditional</animation>
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>540</width>
						<height>460</height>
						<texture background="true" diffuse="views/cycle/diffuse.png">$VAR[Art-Fanart]</texture>
						<aspectratio scalediffuse="false" align="center" aligny="center">scale</aspectratio>
					</control>
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>540</width>
						<height>460</height>
						<texture>views/cycle/border.png</texture>
					</control>
				</control>
			</itemlayout>

			<focusedlayout width="640">
				<control type="group">
					<animation effect="zoom" start="75" end="125" time="350" condition="true" center="270,230">Focus</animation>
					<animation effect="zoom" end="75" start="125" time="350" condition="true" center="270,230">UnFocus</animation>
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>540</width>
						<height>460</height>
						<texture background="true" diffuse="views/cycle/diffuse.png">$VAR[Art-Fanart]</texture>
						<aspectratio scalediffuse="false" align="center" aligny="center">scale</aspectratio>
					</control>
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>540</width>
						<height>460</height>
						<texture>views/cycle/border.png</texture>
					</control>
				</control>
			</focusedlayout>

		</control>


	</include>

</includes>
