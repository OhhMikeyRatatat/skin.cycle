				<effect type="fade" start="0" end="100" time="500" tween="cubic" easing="inout" />
			</animation>
			<animation type="Hidden">
				<effect type="fade" start="100" end="0" time="400" tween="cubic" easing="out" />
			</animation>
		</control>
	</include>
	<include name="Home-BGFade">
		<control type="image">
			<include>FullscreenDimensions</include>
			<texture>common/white.png</texture>
			<colordiffuse>Home-BGFade</colordiffuse>
			<animation type="Visible">
				<effect type="fade" start="0" end="100" time="950" tween="cubic" easing="inout" />
			</animation>
			<animation type="Hidden">
				<effect type="fade" start="100" end="0" time="750" tween="cubic" easing="out" />
			</animation>
		</control>
	</include>
